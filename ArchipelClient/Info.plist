<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CPBundleIdentifier</key>
    <string>org.archipelproject.archipel</string>
    <key>Main cib file base name</key>
    <string>MainMenu.cib</string>
    <key>CPBundleName</key>
    <string>Archipel</string>
    <key>CPPrincipalClass</key>
    <string>CPApplication</string>
    <key>CPBundleIconFile</key>
    <string>icon.png</string>
    
    <key>LPCrashReporterLoggingURL</key>
    <string>http://antoinemercadal.fr/archipelCrashReporting/reports.php</string>
    
    <!-- 
        Local storage information. It can be 
            - TNUserDefaultStorageTypeHTML5
            - TNUserDefaultStorageTypeCookie
            - TNUserDefaultStorageTypeNoStorage
    -->
    <key>TNUserDefaultStorageType</key>
    <string>TNUserDefaultStorageTypeHTML5</string>
    
    <!--
        Url of the welcome page. It can be any URL (online) or "local" to 
        display the defaut welcome page (offline)
    -->
    <key>TNHelpWindowURL</key>
    <string>http://antoinemercadal.fr/archipelhelp/</string>
    
    <key>TNArchipelModuleLoadingDelay</key>
    <string>0.3</string>
    
    
    <key>TNArchipelVersion</key>
    <string>Alpha-0.1 (Beethoven)</string>
    
    <key>TNArchipelCopyright</key>
    <string>Archipel - Copyright Â© 2010 Antoine Mercadal - antoine.mercadal@inframonde.eu</string>
    
</dict>
</plist>
